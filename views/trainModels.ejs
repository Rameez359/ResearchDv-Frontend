                            <script>
                                document.addEventListener("DOMContentLoaded", function () {
                                    function fetchUserProjects(userId) {
                                        const url = `http://localhost:3000/projects/getUserProject/${userId}`;
                                        fetch(url)
                                            .then(response => {
                                                if (!response.ok) {
                                                    throw new Error('Network response was not ok');
                                                }
                                                return response.json();
                                            })
                                            .then(data => {
                                                console.log('User projects:', data.result);
                                                displayUserProjects(data.result);
                                            })
                                            .catch(error => {
                                                console.error('There was an error!', error);

                                            });
                                    }

                                    function displayUserProjects(projects) {
                                        const projectContainer = document.querySelector('#project-container');

                                        projectContainer.innerHTML = '';

                                        for (let project of projects) {
                                            window.showAlert = function (projectId) {
                                                console.log("Project ID: " + projectId);
                                                localStorage.setItem('currentProjectId', `${projectId}`);
                                                window.location.href = '../Source/dashboard.html';

                                            };
                                            const projectDiv = document.createElement('div');
                                            projectDiv.classList.add('project-item');

                                            projectDiv.innerHTML = `
                                      <a id="hello" onclick="showAlert('${project._id}')">
                                      <img src="./images/1.png" alt="Object Detection Image" class=" mb-2">
                                      <input type="hidden" value=${project._id}>
                                      <h2>${project.projectName}</h2>
                                        <p>${project.projectType}</p>
                                       </a>
                                        
                                      `;

                                            projectContainer.appendChild(projectDiv);

                                        };
                                    }

                                    const userId = localStorage.getItem('userId');
                                    fetchUserProjects(userId);
                                });
                            </script>
